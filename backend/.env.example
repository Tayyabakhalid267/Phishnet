# Backend Environment Variables
DEBUG=true
SECRET_KEY=phishnet-super-secret-key-change-in-production-immediately
ACCESS_TOKEN_EXPIRE_MINUTES=30

# Database Configuration
DATABASE_URL=sqlite:///./phishnet.db
# For PostgreSQL: DATABASE_URL=postgresql+asyncpg://user:password@localhost/phishnet

# Redis Configuration
REDIS_URL=redis://localhost:6379

# External API Keys
VIRUSTOTAL_API_KEY=your-virustotal-api-key-here
PHISHTANK_API_KEY=your-phishtank-api-key-here
ABUSEIPDB_API_KEY=your-abuseipdb-api-key-here
GOOGLE_SAFE_BROWSING_KEY=your-google-safe-browsing-key-here
HUGGINGFACE_API_KEY=your-huggingface-api-key-here

# File Upload Configuration
MAX_FILE_SIZE=10485760
UPLOAD_DIR=./uploads
MODEL_CACHE_DIR=./ai_models/cache

# Email Configuration
IMAP_TIMEOUT=30
MAX_EMAIL_SIZE=52428800

# Threat Intelligence
THREAT_INTEL_CACHE_TTL=3600
DOMAIN_REPUTATION_THRESHOLD=0.7

# Rate Limiting
RATE_LIMIT_REQUESTS=100
RATE_LIMIT_WINDOW=60

# Logging
LOG_LEVEL=INFO
LOG_FILE=./logs/phishnet.log

# Blockchain Configuration
BLOCKCHAIN_NETWORK=ethereum
BLOCKCHAIN_PRIVATE_KEY=your-blockchain-private-key-here

# Notification Services
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USER=your-email@gmail.com
SMTP_PASSWORD=your-app-password
SLACK_WEBHOOK_URL=your-slack-webhook-url
TEAMS_WEBHOOK_URL=your-teams-webhook-url